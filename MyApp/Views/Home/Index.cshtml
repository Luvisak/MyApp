@model IEnumerable<MyApp.Models.User>

<h1 class="text-center">User List</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
                <tr>
                    <td>@user.Name</td>
                    <td>@user.Email</td>
                </tr>
        }
    </tbody>
</table>

